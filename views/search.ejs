<div class="auther">
    <h1 class="auther_heading">
    </h1>
</div>
<section class="articles">
    <ul class="article-ul">
        <% data.forEach(post=>{ %>


            <li>
                <a href="/post/<%= post._id %>">
                    <span>
                        <%=post.title %>
                    </span>
                    <span class="article-list__date">
                        <%=post.createdAt.toDateString() %>
                    </span>
                </a>
            </li>
            <% })%>
    </ul>
</section>